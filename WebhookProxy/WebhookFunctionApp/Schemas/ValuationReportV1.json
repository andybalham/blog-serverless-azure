  "title": "Valuation Report",
  "type": "object",
  "properties": {
    "reportId": {
      "type": "string"
    },
    "date": {
      "type": "string",
      "format": "date"
    },
    "propertyAddress": {
      "type": "string"
    },
    "ownerName": {
      "type": "string"
    },
    "estimatedValue": {
      "type": "number"
    },
    "valuationMethod": {
      "type": "string"
    },
    "appraiser": {
      "type": "string"
    },
    "appraiserContact": {
      "type": "string"
    },
    "landArea": {
      "type": "number"
    },
    "buildingArea": {
      "type": "number"
    }
  },
  "required": [
    "reportId",
    "date",
    "propertyAddress",
    "ownerName",
    "estimatedValue",
    "valuationMethod",
    "appraiser",
    "appraiserContact",
    "landArea",
    "buildingArea"
